{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "google",
    "prettier",
    "plugin:jest/recommended",
    "plugin:jest/style"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier","jest"],
  "rules": {
  "prettier/prettier": "error",
  "complexity": ["error", { "max": 3 }],
  "require-jsdoc": "off"
  },
  "ignorePatterns" : [
    ",./*.config.js",
    "*_test.js"
  ]
}