{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true,
    "jest/globals": true
  },
  "extends": [
    "google",
    "prettier",
    "eslint:recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12
  },
  "plugins": ["prettier"],
  "rules": {
    "prettier/prettier": "error",
    "complexity": ["error", { "max": 3 }],
    "require-jsdoc": "off"
  },
  "ignorePatterns" : [
    ",./*.config.js",
    "*_test.js"
  ]
}